@article{dongImpactAdrenomedullinMitochondrial2023,
  title = {Impact of Adrenomedullin on Mitochondrial Respiratory Capacity in Human Adipocyte},
  author = {Dong, Yuanlin and Vipin, Vidyadharan Alukkal and Blesson, Chellakkan Selvanesan and Yallampalli, Chandrasekhar},
  year = 2023,
