@article{schulteMitochondrialComplexomeReveals2023,
  title = {Mitochondrial Complexome Reveals Quality-Control Pathways of Protein Import},
  author = {Schulte, Uwe and Den Brave, Fabian and Haupt, Alexander and Gupta, Arushi and Song, Jiyao and M{\"u}ller, Catrin S. and Engelke, Jeannine and Mishra, Swadha and M{\aa}rtensson, Christoph and Ellenrieder, Lars and Priesnitz, Chantal and Straub, Sebastian P. and Doan, Kim Nguyen and Kulawiak, Bogusz and Bildl, Wolfgang and Rampelt, Heike and Wiedemann, Nils and Pfanner, Nikolaus and Fakler, Bernd and Becker, Thomas},
  year = 2023,
