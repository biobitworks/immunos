@article{girardCryogenicSensorEnabling2023,
  title = {Cryogenic Sensor Enabling Broad-Band and Traceable Power Measurements},
  author = {Girard, J.-P. and Lake, R. E. and Liu, W. and Kokkoniemi, R. and Visakorpi, E. and Govenius, J. and M{\"o}tt{\"o}nen, M.},
