@inproceedings{wanPredictionProLongevityAntiLongevity2013,
  title = {Prediction of the {{Pro-Longevity}} or {{Anti-Longevity Effect}} of {{Caenorhabditis Elegans Genes Based}} on {{Bayesian Classification Methods}}},
