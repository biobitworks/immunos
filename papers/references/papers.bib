@article{chanNovelInsightsMultiomics2022,
  title = {Novel {{Insights}} from a {{Multiomics Dissection}} of the {{Hayflick Limit}}},
  author = {Chan, Michelle and Yuan, Han and Soifer, Ilya and Maile, Tobias M and Wang, Rebecca Y and Ireland, Andrea and O'Brien, Jonathon J and Goudeau, J{\'e}r{\^o}me and Chan, Leanne JG and Vijay, Twaritha and Freund, Adam and Kenyon, Cynthia and Bennett, Bryson D and McAllister, Fiona E and Kelley, David R and Roy, Margaret and Cohen, Robert L and Levinson, Arthur D and Botstein, David and Hendrickson, David G},
  year = 2022,
  month = feb,
  journal = {eLife},
  volume = {11},
  pages = {e70283},
  issn = {2050-084X},
  doi = {10.7554/eLife.70283},
  urldate = {2022-04-13},
  abstract = {The process wherein dividing cells exhaust proliferative capacity and enter into replicative senescence has become a prominent model for cellular aging in vitro. Despite decades of study, this cellular state is not fully understood in culture and even much less so during aging. Here, we revisit Leonard Hayflick's original observation of replicative senescence in WI-\textbackslash -38 human lung fibroblasts equipped with a battery of modern techniques including RNA-s\textbackslash - eq, single-c\textbackslash - ell RNA-\textbackslash -seq, proteomics, metabolomics, and ATAC-\textbackslash -seq. We find evidence that the transition to a senescent state manifests early, increases gradually, and corresponds to a concomitant global increase in DNA accessibility in nucleolar and lamin associated domains. Furthermore, we demonstrate that senescent WI-\textbackslash -38 cells acquire a striking resemblance to myofibroblasts in a process similar to the epithelial to mesenchymal transition (EMT) that is regulated by t YAP1/TEAD1 and TGF-{$\beta$}2. Lastly, we show that verteporfin inhibition of YAP1/TEAD1 activity in aged WI-\textbackslash -38 cells robustly attenuates this gene expression program.}
}

