  title = {Defining the Age-Dependent and Tissue-Specific Circadian Transcriptome in Male Mice},
  author = {Wolff, Christopher A. and {Gutierrez-Monreal}, Miguel A. and Meng, Lingsong and Zhang, Xiping and Douma, Lauren G. and Costello, Hannah M. and Douglas, Collin M. and Ebrahimi, Elnaz and Pham, Ann and Oliveira, Aline C. and Fu, Chunhua and Nguyen, Amy and Alava, Bryan R. and Hesketh, Stuart J. and Morris, Andrew R. and Endale, Mehari M. and Crislip, G. Ryan and Cheng, Kit-yan and Schroder, Elizabeth A. and Delisle, Brian P. and Bryant, Andrew J. and Gumz, Michelle L. and Huo, Zhiguang and Liu, Andrew C. and Esser, Karyn A.},
