@article{fioreLiveImagingExtracellular2025,
  title = {Live Imaging of the Extracellular Matrix with a Glycan-Binding Fluorophore},
  author = {Fiore, Antonio and Yu, Guoqiang and Northey, Jason J. and Patel, Ronak and Ravenscroft, Thomas A. and Ikegami, Richard and Kolkman, Wiert and Kumar, Pratik and Dilan, Tanya L. and Ruetten, Virginia M. S. and Ahrens, Misha B. and Shroff, Hari and Wang, Shaohe and Weaver, Valerie M. and Pedram, Kayvon},
  year = 2025,
