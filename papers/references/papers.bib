@article{zhangSinglecellTranscriptomicLandscape2020,
  title = {A Single-Cell Transcriptomic Landscape of Primate Arterial Aging},
  author = {Zhang, Weiqi and Zhang, Shu and Yan, Pengze and Ren, Jie and Song, Moshi and Li, Jingyi and Lei, Jinghui and Pan, Huize and Wang, Si and Ma, Xibo and Ma, Shuai and Li, Hongyu and Sun, Fei and Wan, Haifeng and Li, Wei and Chan, Piu and Zhou, Qi and Liu, Guang-Hui and Tang, Fuchou and Qu, Jing},
  year = 2020,
