@article{yanAminoAcidSensing2012,
  title = {Amino {{Acid Sensing}} and {{Regulation}} of {{mTORC1}}},
  author = {Yan, Lijun and Lamb, Richard F.},
  year = 2012,
  month = aug,
  journal = {Seminars in Cell \& Developmental Biology},
  volume = {23},
  number = {6},
  pages = {621--625},
  issn = {10849521},
  doi = {10.1016/j.semcdb.2012.02.001},
  urldate = {2020-07-24},
  abstract = {Amino acids play fundamental roles in the cell both as the building blocks of new proteins and as metabolic precursors. To adapt to their limitation during periods of protein starvation, multiple adaptive mechanisms have evolved, including a rapid cessation of new protein synthesis, an increase in amino acid biosynthesis and transport, and autophagy. Here, we discuss what we currently know about how amino acid limitation is sensed, and how this sensing might be transmitted to mTORC1 to regulate protein synthesis and autophagy.}
}

@article{yang2023,
  title = {Pulsed Stimulated {{Brillouin}} Microscopy Enables High-Sensitivity Mechanical Imaging of Live and Fragile Biological Specimens},
  author = {Yang, Fan and Bevilacqua, Carlo and Hambura, Sebastian and Neves, Ana and Gopalan, Anusha and Watanabe, Koki and Govendir, Matt and Bernabeu, Maria and Ellenberg, Jan and {Diz-Mu{\~n}oz}, Alba and K{\"o}hler, Simone and Rapti, Georgia and Jechlinger, Martin and Prevedel, Robert},
