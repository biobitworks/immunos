@article{leeDefiningImpactMutation2019,
  title = {Defining the {{Impact}} of {{Mutation Accumulation}} on {{Replicative Lifespan}} in {{Yeast Using Cancer-Associated Mutator Phenotypes}}},
  author = {Lee, Mitchell B. and Dowsett, Ian T. and Carr, Daniel T. and Wasko, Brian M. and Stanton, Sarah G. and Chung, Michael S. and Ghodsian, Niloufar and Bode, Anna and Kiflezghi, Michael G. and Uppal, Priya A. and Grayden, Katherine A. and Elala, Yordanos C. and Tang, Thao T. and Tran, Ngoc H. B. and Tran, Thu H. B. and Diep, Anh B. and Hope, Michael and Promislow, Daniel E. L. and Kennedy, Scott R. and Kaeberlein, Matt and Herr, Alan J.},
  year = 2019,
