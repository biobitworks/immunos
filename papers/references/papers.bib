  doi = {10.7554/eLife.59201},
  urldate = {2021-11-17},
  abstract = {Robust biomarkers of aging have been developed from DNA methylation in humans and more recently, in mice. This study aimed to generate a novel epigenetic clock in rats--- a model with unique physical, physiological, and biochemical advantages--- by incorporating behavioral data, unsupervised machine learning, and network analysis to identify epigenetic signals that not only track with age, but also relates to phenotypic aging. Reduced representation bisulfite sequencing (RRBS) data was used to train an epigenetic age (DNAmAge) measure in Fischer 344 CDF (F344) rats. This measure correlated with age at (r = 0.93) in an independent sample, and related to physical functioning (p=5.9e-3), after adjusting for age and cell counts. DNAmAge was also found to correlate with age in male C57BL/6 mice (r = 0.79), and was decreased in response to caloric restriction. Our signatures driven by CpGs in intergenic regions that showed substantial overlap with H3K9me3, H3K27me3, and E2F1 transcriptional factor binding.}
}

@article{liAssociationsRegularGlucosamine2020,
  title = {Associations of {{Regular Glucosamine Use}} with {{All-Cause}} and {{Cause-Specific Mortality}}: {{A Large Prospective Cohort Study}}},
  shorttitle = {Associations of {{Regular Glucosamine Use}} with {{All-Cause}} and {{Cause-Specific Mortality}}},
  author = {Li, Zhi-Hao and Gao, Xiang and Chung, Vincent CH and Zhong, Wen-Fang and Fu, Qi and Lv, Yue-Bin and Wang, Zheng-He and Shen, Dong and Zhang, Xi-Ru and Zhang, Pei-Dong and Li, Fu-Rong and Huang, Qing-Mei and Chen, Qing and Song, Wei-Qi and Wu, Xian-Bo and Shi, Xiao-Ming and Kraus, Virginia Byers and Yang, Xingfen and Mao, Chen},
  year = 2020,
  month = jun,
  journal = {Annals of the Rheumatic Diseases},
  volume = {79},
  number = {6},
  pages = {829--836},
  issn = {0003-4967, 1468-2060},
  doi = {10.1136/annrheumdis-2020-217176},
  urldate = {2022-04-13},
  abstract = {Objectives\textbackslash hspace0.6em To evaluate the associations of regular glucosamine use with all-\textbackslash -cause and cause-\textbackslash -specific mortality in a large prospective cohort. Methods\textbackslash hspace0.6em This population-\textbackslash -based prospective cohort study included 495 077 women and men (mean (SD) age, 56.6 (8.1) years) from the UK Biobank study. Participants were recruited from 2006 to 2010 and were followed up through 2018. We evaluated all-c\textbackslash - ause mortality and mortality due to cardiovascular disease (CVD), cancer, respiratory and digestive disease. HRs and 95\% CIs for all-\textbackslash -cause and cause-\textbackslash -specific mortality were calculated using Cox proportional hazards models with adjustment for potential confounding variables. Results\textbackslash hspace0.6em At baseline, 19.1\% of the participants reported regular use of glucosamine supplements. During a median follow-\textbackslash -up of 8.9 years (IQR 8.3-- 9.7 years), 19 882 all-\textbackslash -cause deaths were recorded, including 3802 CVD deaths, 8090 cancer deaths, 3380 respiratory disease deaths and 1061 digestive disease deaths. In multivariable adjusted analyses, the HRs associated with glucosamine use were 0.85 (95\% CI 0.82 to 0.89) for all-\textbackslash -cause mortality, 0.82 (95\% CI 0.74 to 0.90) for CVD mortality, 0.94 (95\% CI 0.88 to 0.99) for cancer mortality, 0.73 (95\% CI 0.66 to 0.81) for respiratory mortality and 0.74 (95\% CI 0.62 to 0.90) for digestive mortality. The inverse associations of glucosamine use with all-\textbackslash -cause mortality seemed to be somewhat stronger among current than non-\textbackslash -current smokers (p for interaction=0.00080). Conclusions\textbackslash hspace0.6em Regular glucosamine supplementation was associated with lower mortality due to all causes, cancer, CVD, respiratory and digestive diseases.}
}

