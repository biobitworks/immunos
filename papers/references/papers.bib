@article{timmermannHumanBrainEffects2023,
  title = {Human Brain Effects of {{DMT}} Assessed via {{EEG-fMRI}}},
  author = {Timmermann, Christopher and Roseman, Leor and Haridas, Sharad and Rosas, Fernando E. and Luan, Lisa and Kettner, Hannes and Martell, Jonny and Erritzoe, David and Tagliazucchi, Enzo and Pallavicini, Carla and Girn, Manesh and Alamia, Andrea and Leech, Robert and Nutt, David J. and {Carhart-Harris}, Robin L.},
