@article{levineRatEpigeneticClock2020,
  title = {A {{Rat Epigenetic Clock Recapitulates Phenotypic Aging}} and {{Co-Localizes}} with {{Heterochromatin}}},
  author = {Levine, Morgan and McDevitt, Ross A and Meer, Margarita and Perdue, Kathy and Di Francesco, Andrea and Meade, Theresa and Farrell, Colin and Thrush, Kyra and Wang, Meng and Dunn, Christopher and Pellegrini, Matteo and {de Cabo}, Rafael and Ferrucci, Luigi},
