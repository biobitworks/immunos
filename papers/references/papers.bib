@article{degenStructuralBasisNINJ1mediated2023,
  title = {Structural Basis of {{NINJ1-mediated}} Plasma Membrane Rupture in Cell Death},
  author = {Degen, Morris and Santos, Jos{\'e} Carlos and Pluhackova, Kristyna and Cebrero, Gonzalo and Ramos, Saray and Jankevicius, Gytis and Hartenian, Ella and Guillerm, Undina and Mari, Stefania A. and Kohl, Bastian and M{\"u}ller, Daniel J. and Schanda, Paul and Maier, Timm and Perez, Camilo and Sieben, Christian and Broz, Petr and Hiller, Sebastian},
  year = 2023,
  month = jun,
  journal = {Nature},
  volume = {618},
  number = {7967},
  pages = {1065--1071},
  issn = {0028-0836, 1476-4687},
  doi = {10.1038/s41586-023-05991-z},
  urldate = {2023-07-18},
  abstract = {Abstract                            Eukaryotic cells can undergo different forms of programmed cell death, many of which culminate in plasma membrane rupture as the defining terminal event               1--7               . Plasma membrane rupture was long thought to be driven by osmotic pressure, but it has recently been shown to be in many cases~an active process, mediated by the protein ninjurin-1               8               (NINJ1). Here we resolve the structure of NINJ1 and the mechanism by which it ruptures membranes. Super-resolution microscopy reveals that NINJ1 clusters into structurally diverse assemblies in the membranes of dying cells, in particular large, filamentous assemblies with branched morphology. A cryo-electron microscopy structure of NINJ1 filaments shows a tightly packed fence-like array of transmembrane {$\alpha$}-helices. Filament directionality and stability is defined by two amphipathic {$\alpha$}-helices that interlink adjacent filament subunits. The NINJ1 filament features a hydrophilic side and a hydrophobic side, and molecular dynamics simulations show that it can stably cap membrane edges. The function of the resulting supramolecular arrangement was validated by site-directed mutagenesis. Our data thus suggest that, during lytic cell death, the extracellular {$\alpha$}-helices of NINJ1 insert into the plasma membrane to polymerize NINJ1 monomers into amphipathic filaments that rupture the plasma membrane. The membrane protein NINJ1 is therefore an interactive component of the eukaryotic cell membrane that functions as an in-built breaking point in response to activation of cell death.},
  langid = {english},
  file = {/Users/byron/Zotero/storage/7EC8B56X/Degen et al. - 2023 - Structural basis of NINJ1-mediated plasma membrane.pdf}
}

@article{degreyPracticalityOtherwiseBiomedical2014,
  title = {The {{Practicality}} or {{Otherwise}} of {{Biomedical Rejuvenation Therapies}}: {{A Response}} to {{Kyriazis}}},
  shorttitle = {The {{Practicality}} or {{Otherwise}} of {{Biomedical Rejuvenation Therapies}}},
  author = {{de Grey}, Aubrey D.N.J.},
  year = 2014,
  month = aug,
  journal = {Rejuvenation Research},
  volume = {17},
  number = {4},
  pages = {397--400},
  issn = {1549-1684, 1557-8577},
  doi = {10.1089/rej.2014.1599},
  urldate = {2020-07-24},
  abstract = {Elsewhere in this issue, Marios Kyriazis articulates a sharply skeptical assessment of the feasibility of medical rejuvenation, otherwise known as the ``damage repair'' approach to postponing the ill health of old age. His critique incorporates many errors of fact and/or interpretation and/or logic, which I enumerate here. I recognize that many of these errors are by no means Kyriazis's alone but are shared by numerous observers across the full range of biological expertise. It is for that reason above all that I hope this exchange will be of value to readers. Additionally, Kyriazis raises a number of valid concerns that have not been systematically addressed, by myself or by others, in past publications; I will attempt to remedy that here. For clarity, I will not structure this response as an essay, but instead as a simple point-by-point list of comments concerning individual statements and inferences that Kyriazis makes.}
}

@article{dehouxProbingSinglecellMechanics2015,
  title = {Probing Single-Cell Mechanics with Picosecond Ultrasonics},
  author = {Dehoux, Thomas and Abi Ghanem, Maroun and Zouani, Omar F. and Ducousso, Mathieu and Chigarev, Nikolay and Rossignol, Cl{\'e}ment and Tsapis, Nicolas and Durrieu, Marie-Christine and Audoin, Bertrand},
  year = 2015,
  month = feb,
  journal = {Ultrasonics},
  volume = {56},
  pages = {160--171},
  issn = {0041624X},
  doi = {10.1016/j.ultras.2014.07.010},
  urldate = {2025-10-01},
  abstract = {The mechanical properties of cells play a key role in several fundamental biological processes, such as migration, proliferation, differentiation and tissue morphogenesis. The complexity of the inner cell composition and the intricate meshwork formed by transmembrane cell-substrate interactions demands a non-invasive technique to probe cell mechanics and cell adhesion at a subcell scale. In this paper we review the use of laser-generated GHz acoustic waves---a technique called picosecond ultrasonics (PU)---to probe the mechanical properties of single cells. We first describe applications to vegetal cells and biomimetic systems. We show how these systems can be used as simple models to understand more complex animal cells. We then present an opto-acoustic bio-transducer designed for in vivo measurements in physiological conditions. We illustrate the use of this transducer through the simultaneous probing of the density and compressibility of Allium cepa cells. Finally, we demonstrate that this technique can quantify animal-cell adhesion on metallic surfaces by analyzing the acoustic pulses reflected off the cell-metal interface. This innovative approach allows investigating quantitatively cell mechanics without fluorescent labels or mechanical contact to the cell.},
  langid = {english},
  file = {/Users/byron/Zotero/storage/4PRNSEHN/Dehoux et al. - 2015 - Probing single-cell mechanics with picosecond ultrasonics.pdf}
}

@article{delutyInfluenceGenderInheritance2015,
  title = {The {{Influence}} of {{Gender}} on {{Inheritance}} of {{Exceptional Longevity}}},
  author = {Deluty, Jennifer A. and Atzmon, Gil and Crandall, Jill and Barzilai, Nir and Milman, Sofiya},
  year = 2015,
  month = jun,
  journal = {Aging},
  volume = {7},
  number = {6},
  pages = {412--418},
  issn = {1945-4589},
  doi = {10.18632/aging.100763},
  urldate = {2020-07-24},
  abstract = {While the search for genetic contributors to exceptional longevity has yielded candidates, gender differences in inheritance have generally not been considered. The aim of this study was to investigate gender specific differences in the inheritance of exceptional longevity. Using a standardized questionnaire, we assessed the parental ages of death of Ashkenazi Jews with exceptional longevity and their spouses without exceptional longevity, who served as controls (n=1,114). Mothers of centenarian males and females had significantly longer lifespans compared to the mothers of noncentenarians, 79.0 \textbackslash pm 13.4 vs. 73.0 \textbackslash pm 16.3 years, p{$<$}0.01 and 75.7 \textbackslash pm 15.8 vs. 70.5 \textbackslash pm 18.0 years, p=0.02, respectively. There was also a trend toward longer lifespan among the fathers of centenarian men compared to the lifespan of fathers of noncentenarian men, 73.5 \textbackslash pm 17.0 vs. 69.5 \textbackslash pm15.0 years, p=0.07. The lifespan did not differ between the fathers of centenarian and non-centenarian daughters. Logistic regression models revealed that the odds of being a centenarian for the female and male offspring increased by 21\% and 31\%, respectively, for every additional 10 years of life achieved by the mother (p{$<$}0.05). These findings support a gender-specific inheritance pattern of human longevity and may help focus the search for longevity genes.}
}

@article{demidenkoRejuvantPotentialLifeExtending2021,
  title = {{{Rejuvant}}\textregistered, a {{Potential Life-Extending Compound Formulation}} with {{Alpha-Ketoglutarate}} and {{Vitamins}}, {{Conferred}} an {{Average}} 8 {{Year Reduction}} in {{Biological Aging}}, after an {{Average}} of 7 {{Months}} of {{Use}}, in the {{TruAge DNA Methylation Test}}},
  author = {Demidenko, Oleksandr and Barardo, Diogo and Budovskii, Valery and Finnemore, Robb and Palmer, Francis R. and Kennedy, Brian K. and Budovskaya, Yelena V.},
  year = 2021,
  month = nov,
  journal = {Aging},
  volume = {13},
  number = {22},
  pages = {24485--24499},
  issn = {1945-4589},
  doi = {10.18632/aging.203736},
  urldate = {2022-04-13},
  abstract = {The search continues for possible interventions that delay and/or reverse biological aging, resulting in extended healthspan and lifespan. Interventions delaying aging in animal models are well established; however, most lack validation in humans. The length of human lifespan makes it impractical to perform survival analysis. Instead, aging biomarkers, such as DNA methylation (DNAm) clocks, have been developed to monitor biological age. Herein we report a retrospective analysis of DNA methylation age in 42 individuals taking Rejuvant\textregistered, an alpha-ketoglutarate based formulation, for an average period of 7 months. DNAm testing was performed at baseline and by the end of treatment with Rejuvant\textregistered{} supplementation. Remarkably, individuals showed an average decrease in biological aging of 8 years (p-value=6.538x10-12). Furthermore, the supplementation with Rejuvant\textregistered{} is robust to individual differences, as indicated by the fact that a large majority of participants decreased their biological age. Moreover, we found that Rejuvant\textregistered{} is of additional benefit to chronologically and biologically older individuals. While continued testing, particularly in a placebo-controlled design, is required, the nearly 8-year reversal in the biological age of individuals taking Rejuvant\textregistered{} for 4 to 10 months is noteworthy, making the natural product cocktail an intriguing candidate to affect human aging.}
}

@article{depuydtSinglecellCellReceptor2023,
  title = {Single-Cell {{T}} Cell Receptor Sequencing of Paired Human Atherosclerotic Plaques and Blood Reveals Autoimmune-like Features of Expanded Effector {{T}} Cells},
  author = {Depuydt, Marie A. C. and Schaftenaar, Frank H. and Prange, Koen H. M. and Boltjes, Arjan and Hemme, Esmeralda and Delfos, Lucie and De Mol, Jill and De Jong, Maaike J. M. and Bernab{\'e} Kleijn, Mireia N. A. and Peeters, Judith A. H. M. and Goncalves, Lauren and Wezel, Anouk and Smeets, Harm J. and De Borst, Gert J. and Foks, Amanda C. and Pasterkamp, Gerard and De Winther, Menno P. J. and Kuiper, Johan and Bot, Ilze and Sl{\"u}tter, Bram},
