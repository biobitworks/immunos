@article{mayeliParietooccipitalERPIndicators2023,
  title = {Parieto-Occipital {{ERP}} Indicators of Gut Mechanosensation in Humans},
  author = {Mayeli, Ahmad and Al Zoubi, Obada and White, Evan J. and Chappelle, Sheridan and Kuplicki, Rayus and Morton, Alexa and Bruce, Jaimee and Smith, Ryan and Feinstein, Justin S. and Bodurka, Jerzy and Paulus, Martin P. and Khalsa, Sahib S.},
