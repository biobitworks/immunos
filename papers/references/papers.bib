@article{bujarrabal-duesoDREAMComplexFunctions2023,
  title = {The {{DREAM}} Complex Functions as Conserved Master Regulator of Somatic {{DNA-repair}} Capacities},
  author = {{Bujarrabal-Dueso}, Arturo and Sendtner, Georg and Meyer, David H. and Chatzinikolaou, Georgia and Stratigi, Kalliopi and Garinis, George A. and Schumacher, Bj{\"o}rn},
