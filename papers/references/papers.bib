  file = {/Users/byron/Zotero/storage/3WP5L9ZM/Nobile and Johnson - 2015 - Candida albicans Biofilms and Human Disease.pdf}
}

@article{nozakiCondensedLiquidlikeDomain2023,
  title = {Condensed but Liquid-like Domain Organization of Active Chromatin Regions in Living Human Cells},
  author = {Nozaki, Tadasu and Shinkai, Soya and Ide, Satoru and Higashi, Koichi and Tamura, Sachiko and Shimazoe, Masa A. and Nakagawa, Masaki and Suzuki, Yutaka and Okada, Yasushi and Sasai, Masaki and Onami, Shuichi and Kurokawa, Ken and Iida, Shiori and Maeshima, Kazuhiro},
