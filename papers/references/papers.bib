@article{androvicSpatialTranscriptomicscorrelatedElectron2023,
  title = {Spatial {{Transcriptomics-correlated Electron Microscopy}} Maps Transcriptional and Ultrastructural Responses to Brain Injury},
  author = {Androvic, Peter and Schifferer, Martina and Perez Anderson, Katrin and {Cantuti-Castelvetri}, Ludovico and Jiang, Hanyi and Ji, Hao and Liu, Lu and Gouna, Garyfallia and Berghoff, Stefan A. and {Besson-Girard}, Simon and Knoferle, Johanna and Simons, Mikael and Gokce, Ozgun},
